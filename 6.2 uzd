def calculate_used_mb(partitions):
    result = []
    
    for p in partitions:
        parts = p.split(";")
        
        label = parts[0]
        total_mb = int(parts[2])
        used_percent = int(parts[3])
        
        used_mb = int(total_mb * (used_percent / 100))
        
        result.append({
            "Label": label,
            "UsedMB": used_mb
        })
        
    return result


# Пример входных данных
partitions = [
    "Disk1;NTFS;1000;50",
    "Disk2;FAT32;2000;25",
    "Disk3;EXT4;500;80"
]

data = calculate_used_mb(partitions)

for item in data:
    print(item)
